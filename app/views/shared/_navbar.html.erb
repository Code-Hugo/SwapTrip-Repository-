<link href="../assets/dist/css/bootstrap.min.css" rel="stylesheet">
  <header class="navbar d-flex flex-wrap align-items-center justify-content-center justify-content-md-between py-3">
  <div class="container">
      <%= link_to flight_tickets_path, class: "navbar-brand" do %>
        <%= image_tag "swaptrip_logo_white_1.png", class: "logo" %>
      <% end %>

      <ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
        <li><a href="/flight_tickets" class="nav-link px-2 link-secondary">Home</a></li>
        <li><a href="/flight_tickets/new" class="nav-link px-2 link-dark">Flight Form</a></li>
        <% if user_signed_in? %>
        <li><a href="/my_dashboard" class="nav-link px-2 link-dark">My Dashboard</a></li>
          <% else %>
        <li><a href="/users/sign_in" class="nav-link px-2 link-dark">My Dashboard</a></li>
        <% end %>
        <li><a href="#" class="nav-link px-2 link-dark">About us</a></li>
        <li><a href="#" class="nav-link px-2 link-dark">Contact</a></li>
      </ul>

      <div class="buttons col-md-3 text-end">
        <% if user_signed_in? %>
          <%= link_to "Log-out", destroy_user_session_path, method: "delete", class: "signup btn btn-primary" %>
          <%#= redirect_to root_path %>
        <% else %>
          <%= link_to "Login", new_user_session_path, class: "login btn btn-outline-primary me-2" %>
          <%= link_to "Sign_up", new_user_registration_path, class: "signup btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </header>
